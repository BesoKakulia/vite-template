{
	"tabWidth": 2,
	"useTabs": true,
	"printWidth": 80,
	"semi": true,
	"trailingComma": "es5",
	"jsxSingleQuote": false,
	"singleQuote": true,
	"endOfLine": "auto",
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"^@/(.*)$",
		"^[./].*(?<!scss|css)$",
		"^./*.*.(css|scss)"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
